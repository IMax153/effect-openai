{
  "name": "@effect/openai",
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@8.12.1",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "description": "An OpenAI client written with Effect",
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/effect-ts/openai.git"
  },
  "author": "Maxwell Brown<maxwellbrown1990@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/effect-ts/openai/issues"
  },
  "homepage": "https://github.com/effect-ts/openai",
  "scripts": {
    "prepare": "pnpm clean && pnpm build",
    "update-version": "changeset version && pnpm install --no-frozen-lockfile && node ./scripts/version.mjs",
    "lint": "eslint src test",
    "lint-fix": "eslint src test --fix",
    "coverage": "vitest run --coverage",
    "check": "tsc -b tsconfig.json",
    "test": "vitest",
    "clean": "rimraf dist coverage .tsbuildinfo",
    "build": "tsup"
  },
  "dependencies": {
    "@effect/cli": "^0.32.2",
    "@effect/experimental": "^0.8.10",
    "@effect/opentelemetry": "^0.30.14",
    "@effect/platform": "^0.43.11",
    "@effect/platform-node": "^0.42.11",
    "@effect/schema": "^0.61.7",
    "@opentelemetry/exporter-prometheus": "^0.48.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.48.0",
    "@opentelemetry/sdk-trace-base": "^1.21.0",
    "@opentelemetry/sdk-trace-node": "^1.21.0",
    "@sqlfx/sql": "^0.43.2",
    "@sqlfx/sqlite": "^0.43.2",
    "better-sqlite3": "^9.4.0",
    "effect": "2.2.5",
    "openai": "^4.26.1",
    "sqlite-vss": "^0.1.2",
    "tiktoken-node": "^0.0.7"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.1",
    "@effect/eslint-plugin": "^0.1.2",
    "@effect/language-service": "^0.1.0",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^1.2.2",
    "eslint": "^8.56.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-codegen": "^0.23.0",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sort-destructure-keys": "^1.5.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vitest": "^1.2.2"
  }
}
